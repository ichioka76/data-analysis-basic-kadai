      "source": [
        "import numpy as np\n",
        "\n",
        "A = np.array([[0,1],[2,3],[4,5]])\n",
        "B = np.array([[0,1,2,3],[4,5,6,7]])\n",
        "\n",
        "result = np.dot(A,B)\n",
        "\n",
        "print(\"行列積の結果：\")\n",
        "print(result)\n",
        "\n",
        "max_value = np.max(result)\n",
        "\n",
        "print(\"行列積の結果の最大値:\", max_value)"
      ]
    }
